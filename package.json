{
  "name": "alphaaistockx",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build && node scripts/post-build-patch.cjs",
    "start": "next start",
    "export": "next build && next export",
    "netlify-build": "npm run build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "check-all": "npm run format:check && npm run lint && npm run type-check",
    "fix-all": "npm run format && npm run lint:fix",
    "clean": "rm -rf .next out node_modules/.cache",
    "docker:up": "export ALPACA_API_KEY=your_key && export ALPACA_SECRET_KEY=your_secret && docker-compose up --build",
    "analyze": "ANALYZE=true npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "security:check": "npm run security:audit && npm run lint && npm run type-check",
    "ai:fix-imports": "node fix-all-ui-imports.js",
    "ai:fix-cards": "node fix-card-imports.js",
    "ai:fix-missing": "node fix-missing-cards.js",
    "ai:fix-syntax": "node fix-critical-syntax-errors.js",
    "ai:power-up": "npm run ai:fix-imports && npm run ai:fix-cards && npm run ai:fix-missing && npm run ai:fix-syntax",
    "trading:start": "node ai_trader/index.js",
    "trading:backtest": "node ai_trader/backtest.js",
    "trading:simulate": "node ai_trader/simulator.js",
    "ai:train": "node ai_trader/train-model.js",
    "ai:optimize": "node ai_trader/optimize-strategies.js",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:github": "gh-pages -d out",
    "postinstall": "npm run ai:power-up"
  },
